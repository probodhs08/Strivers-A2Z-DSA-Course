class Solution{
public:
    int evenlyDivides(int N){
        //code here
        int count=0;
        int k=N;
        int x;
        while(N>0){
            x=N%10;
            if(x!=0 && k%x==0){
                count++;
            }
            N=N/10;
        }
        return count;
    }
};
