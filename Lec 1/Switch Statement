class Solution {
  public:
    double switchCase(int choice, vector<double> &arr) {
        // code here
        double pi = 2*acos(0.0);
        switch(choice){
            case 1:
            return pi *arr[0]*arr[0];
            break;
            case 2:
            return arr[0]*arr[1];
        }
    }
};
